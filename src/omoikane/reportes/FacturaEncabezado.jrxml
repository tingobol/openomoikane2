     ventas.`impuestos` AS ventas_impuestos,
     ventas.`total` AS ventas_total,
     clientes.`id_cliente` AS clientes_id_cliente,
     clientes.`RFC` AS clientes_RFC,
     clientes.`razonSocial` AS clientes_razonSocial,
     clientes.`direccion` AS clientes_direccion,
     clientes.`telefono` AS clientes_telefono,
     clientes.`cp` AS clientes_cp,
     ventas.`id_cliente` AS ventas_id_cliente
FROM
     `clientes` clientes INNER JOIN `ventas` ventas ON clientes.`id_cliente` = $P{cliente}
WHERE
     id_venta = $P{IDVenta}]]>
